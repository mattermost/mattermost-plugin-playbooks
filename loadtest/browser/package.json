{
  "name": "mattermost-plugin-playbooks-loadtest-browser",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:bundle && npm run types:generate",
    "build:bundle": "node scripts/generate_bundle.mjs",
    "types:check": "tsc --noEmit",
    "types:generate": "node scripts/generate_types.mjs",
    "lint:check": "eslint src --quiet && prettier --check \"src/**/*.ts\"",
    "lint:fix": "eslint src --quiet --fix && prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf node_modules dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "25.0.9",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "peerDependencies": {
    "@mattermost/loadtest-browser-lib": "file:../../../mattermost-load-test-ng/browser/src/lib",
    "@mattermost/playwright-lib": "^11.3.0",
    "@playwright/test": "^1.57.0"
  }
}
